package tlsmem

const (
	OfsHandshakeSecret = 12
	OfsClientKey       = OfsHandshakeSecret + 32
	OfsClientIV        = OfsClientKey + 16
	OfsServerKey       = OfsClientIV + 12
	OfsServerIV        = OfsServerKey + 16
	OfsFinishedKey     = OfsServerIV + 12
	OfsClientSeq       = OfsFinishedKey + 32
	OfsServerSeq       = OfsClientSeq + 8
	Size               = OfsServerSeq + 8
)
