package kernel

type PC = uint16

// Syscall defines the system calls.
type Syscall = uint8

const (
	// SysExit exits the process.
	SysExit      = 1
	SysFork      = 2
	SysPeek      = 3
	SysRead      = 4
	SysSkip      = 5
	SysWrite     = 6
	SysOpen      = 7
	SysClose     = 8
	SysGetrandom = 9
)
