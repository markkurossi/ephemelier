package main

import (
	"crypto/cipher/gcm"
	"ephemelier/kernel"
)

func main(g kernel.Garbler, e kernel.Evaluator) (
	[]byte, kernel.PC, kernel.Syscall, int32, []byte, int32) {

	if g.arg0 < 0 {
		return nil, 0, kernel.SysExit, 1, nil, 0
	}

	return nil, intern(StReadResult), kernel.SysRead, 0, nil, 256
}
