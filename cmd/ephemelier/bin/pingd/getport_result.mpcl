package main

import (
	"ephemelier/kernel"
)

func main(g, e kernel.PeerResult) (
	[]byte, kernel.PC, kernel.Syscall, int32, []byte, int32) {

	if g.arg0 < 0 {
		return nil, 0, kernel.SysExit, 1, nil, 0
	}

	return nil, intern(StReceiveResult), kernel.SysRead, g.arg0, nil, 1024
}
