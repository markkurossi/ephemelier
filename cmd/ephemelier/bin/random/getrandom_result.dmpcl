package main

import (
	"ephemelier/kernel"
)

func main(g kernel.G, e kernel.E) ([]byte, kernel.PC, kernel.Syscall, int32) {

	if g.arg0 < 0 || e.arg0 < 0 {
		return nil, 0, kernel.SysExit, 1
	}
	return nil, intern(StFormat), kernel.SysYield, 0
}
