package main

import (
	"ephemelier/fs"
	"ephemelier/kernel"
	"ephemelier/memory/semihonest"
)

func main(g, e kernel.PeerResult) (
	[]byte, kernel.PC, kernel.Syscall, int32, []byte, int32) {

	if g.arg0 < 0 {
		return nil, 0, kernel.SysExit, g.arg0, nil, 0
	}

	return nil, intern(StReadkeyResult), kernel.SysRead, g.arg0, nil, 32
}
