package main

import (
	"encoding/binary"
	"ephemelier/kernel"
)

func main(g, e kernel.PeerResult) (
	[]byte, kernel.PC, kernel.Syscall, int32, []byte, int32) {

	if g.arg0 < 0 {
		return nil, 0, kernel.SysExit, 1, nil, 0
	}

	prompt := []byte("esh $ ")

	return nil, intern(StPromptResult), kernel.SysWrite, 1, prompt, len(prompt)
}
