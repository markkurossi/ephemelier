package main

import (
	"encoding/binary"
	"ephemelier/kernel"
	"strconv"
)

func main(g kernel.Garbler1B, e kernel.Evaluator1B) (
	[]byte, kernel.PC, kernel.Syscall, int32, []byte, int32) {

	var retval int32
	if g.arg0 < 0 {
		retval = 1
	}

	return nil, 0, kernel.SysExit, retval, nil, 0
}
