package main

import (
	"bytes"
	"ephemelier/kernel"
	"strconv"
	"unicode"
)

func main(g, e kernel.PeerResult) ([]byte, kernel.PC, kernel.Syscall, int32) {

	if g.arg0 < 0 {
		return nil, 0, kernel.SysExit, 1
	}

	// Get port to pingd.
	return nil, intern(StGetportResult), kernel.SysGetport, g.arg0
}
