package main

import (
	"ephemelier/kernel"
)

func main(g kernel.G1B, e kernel.E1B) (
	[]byte, kernel.PC, kernel.Syscall, int32) {

	socketfd := binary.GetUint32(g.mem[0:])

	return nil, intern(StMainAcceptResult), kernel.SysAccept, int32(socketfd)
}
