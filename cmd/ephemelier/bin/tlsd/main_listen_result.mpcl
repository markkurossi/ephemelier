package main

import (
	"ephemelier/kernel"
	"ephemelier/tlsmem"
)

func main(g kernel.G1B, e kernel.E1B) (
	[]byte, kernel.PC, kernel.Syscall, int32) {

	if g.arg0 < 0 {
		return nil, 0, kernel.SysExit, 1
	}

	mem := binary.PutUint32(g.mem, 0, uint32(g.arg0))

	return mem, intern(StMainAccept), kernel.SysContinue, 0
}
