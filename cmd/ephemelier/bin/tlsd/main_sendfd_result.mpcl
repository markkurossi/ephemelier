package main

import (
	"ephemelier/kernel"
)

func main(g kernel.G1B, e kernel.E1B) (
	[]byte, kernel.PC, kernel.Syscall, int32) {

	fd := binary.GetUint32(g.mem[4:])

	if g.arg0 < 0 {
		return nil, intern(StMainAccept), kernel.SysClose, int32(fd)
	}

	return nil, intern(StMainAccept), kernel.SysContinue, 0
}
