package main

import (
	"ephemelier/kernel"
)

func main(g, e kernel.PeerInit) ([]byte, kernel.PC, kernel.Syscall, int32) {
	if g.arg0 < 0 {
		return nil, 0, kernel.SysExit, 1
	}
	return nil, intern(StChildRecvfdResult), kernel.SysRecvfd, g.arg0
}
