package main

import (
	"ephemelier/kernel"
	"ephemelier/memory"
	"ephemelier/memory/semihonest"
	"ephemelier/tlsmem"
)

func main(g, e memory.PeerInit) ([]byte, kernel.PC, kernel.Syscall, int32) {
	if g.arg0 < 0 {
		return nil, 0, kernel.SysExit, 1
	}

	return nil, intern(StChildGetportResult), kernel.SysGetport, g.arg0
}
