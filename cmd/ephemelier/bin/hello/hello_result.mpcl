package main

import (
	"ephemelier/kernel"
)

func main(g, e kernel.PeerResult) (
	[]byte, kernel.PC, kernel.Syscall, int32, []byte, int32) {

	var retval int32
	if g.arg0 < 0 {
		retval = 1
	}

	return nil, 0, kernel.SysExit, retval, nil, 0
}
